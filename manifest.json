{
    "name": "System FR Extension",
    "version": "2.0",
    "icons": {
        "180": "icon.png"
    },
    "background": {
        "service_work": "background/background.html"
    },
    "description": "Preenchimento automático para tarefas do sistema Fábio Ribeiro Advogados.",
    "action": {
        "default_title": "System FR Extension",
        "default_popup": "popup/index.html"
    },
    "content_scripts": [
        {
            "matches": [
                "http://fabioribeiro.eastus.cloudapp.azure.com/*",
                "https://maps.googleapis.com/*"
            ],
            "js": [
                "chromeUtils.js",
                "content.js"
            ]
        }
    ],
    "permissions": [
        "tabs",
        "storage",
        "webRequest"
    ],
    "manifest_version": 3
}